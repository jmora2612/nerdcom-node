{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "yes/status/(.*)": "<rootDir>/../libs/status/src/$1",
    "yes/status": "<rootDir>/../libs/status/src"
  }
}